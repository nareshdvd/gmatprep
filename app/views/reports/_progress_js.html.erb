<script type="text/javascript">
  var container = $("#progress-graph")[0];
  Highcharts.chart(container, {
    chart:{
      type: 'spline',
      backgroundColor: chart_background
    },
    rangeSelector: {
      selected: 1
    },
    title: {
      text: 'Progress chart'
    },
    xAxis: {
      categories: JSON.parse("<%= (1..41).to_a.to_json %>")
    },
    plotOptions: {
      spline: {
        lineWidth: 2,
        states: {
          hover: {
            lineWidth: 3
          }
        },
        marker: {
          enabled: false
        },
        color: bar_colors[1][0]
      }
    },
    series: [{
      name: 'Progress',
      data: JSON.parse("<%=raw @paper.get_chart_data.to_json %>"),
      type: 'spline',
      tooltip: {
        valueDecimals: 2
      }
    }]
  });
</script>