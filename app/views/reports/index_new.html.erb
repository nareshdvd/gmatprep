<style type="text/css">
  .graph-heading{
    text-align: center;
    font-weight: 600;
  }
  .font-24{
    font-size: 22px;
  }
  .font-22{
    font-size: 22px;
  }
  .font-20{
    font-size: 20px;
  }
  .font-18{
    font-size: 18px;
  }
  .font-16{
    font-size: 16px;
  }
  .font-14{
    font-size: 14px;
  }
  .graph-container{
    margin-top: 10px;
    border-bottom: 1px solid #aeaeae;
    margin-bottom: 60px;
  }
  .graph-heading{
    margin-bottom: 30px;
    border-bottom: 1px solid #aeaeae;
    padding-bottom: 10px;
  }
  .border-light-dark{
    border-color: #aeaeae;
  }
  .border-1{
    border-width: 1px;
    border-style: solid;
  }
  .border-b-none{
    border-bottom: 0px;
  }
  body{
    padding-bottom: 100px !important;
  }
  .highcharts-credits{
    display: none !important;
  }

  .graph-right .content{
    min-height: 380px;
    border: 1px solid #aeaeae;
    border-bottom: none;
    padding: 10px;
  }
  .graph-left .content{
    min-height: 380px;
    border: 1px solid #aeaeae;
    border-bottom: none;
    padding: 10px;
    text-align: justify;
  }
</style>
<script type="text/javascript">
  var bar_colors = JSON.parse('<%=raw @paper.bar_colors.to_json.html_safe %>');
  var chart_background = 'rgba(255, 255, 255, 0.1)';
  var label_inner_size = "11px";
</script>
<%= render "reports/verbal_section_performance" %>
<%= render "reports/sub_section_time_management" %>
<%= render "reports/percent_correct_incorrect" %>
<%= render "reports/progress" %>
<% @section_correct_incorrect_data = @paper.section_percentage_correct_incorrect_data %>
<% @average_time_per_question = @paper.average_time_per_question %>
<%= render "reports/section_correct_incorrect" %>
<%= render "reports/average_time_per_question" %>
<%= render "reports/average_time_correct_incorrect" %>
<%= render "reports/average_difficulty_level" %>
<%= content_for :bottom_js do %>
  <% javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.6.0/Chart.bundle.min.js" %>
  <%= javascript_include_tag "https://code.highcharts.com/highcharts.src.js" %>
  <% render "reports/verbal_section_performance_js" %>
  <%= render "reports/verbal_section_performance_js_new" %>
  <%= render "reports/sub_section_time_management_js" %>
  <%= render "reports/percent_correct_incorrect_js" %>
  <%= render "reports/progress_js" %>
  <% @section_correct_incorrect_data.each do |section_data| %>
    <% sec_name_id = section_data["section_name"].split(" ").join("-") %>
    <%= render "reports/section_correct_incorrect_js", {sec_name: section_data["section_name"], sec_name_id: sec_name_id, section_data: section_data["data"]} %>
  <% end %>
  <script type="text/javascript">
    <% @average_time_per_question.each do |section_data| %>
      <% sec_name_id = section_data["section_name"].split(" ").join("-") %>
      <%= render partial: 'reports/average_time_per_question', locals: {sec_name: section_data['section_name'], sec_name_id: sec_name_id, section_data: section_data['data']}, formats: [:js] %>
    <% end %>
    <%= render partial: 'reports/average_time_correct_incorrect', formats: [:js] %>
    <%= render partial: 'reports/average_difficulty_level', formats: [:js] %>
  </script>
<% end %>