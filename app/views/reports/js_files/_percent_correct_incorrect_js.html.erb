<script type="text/javascript">
  var container = $("#percent-correct-incorrect-graph")[0];
  var data = JSON.parse('<%= @correct_incorrect_graph_data.to_json.html_safe %>');
  Highcharts.chart(container, {
    chart: {
        type: 'column',
        backgroundColor: chart_background,
        margin: 1,
    },
    title: {
        text: ''
    },
    xAxis: {
        categories: ['I Set', 'II Set', 'III Set', 'IV Set'],
        gridLineColor: '#eee',
        lineColor: line_color,
        lineWidth: 1,
    },
    yAxis: {
        min: 0,
        max: 100,
        tickInterval: 20,
        title: {
            text: ''
        },
        stackLabels: {
            enabled: true,
            style: {
                fontWeight: 'bold',
                color: (Highcharts.theme && Highcharts.theme.textColor) || 'gray'
            }
        },
        gridLineColor: '#eee',
        lineColor: line_color,
        lineWidth: 1,
    },
    legend: {
        align: 'right',
        x: -30,
        verticalAlign: 'top',
        y: 25,
        floating: true,
        backgroundColor: (Highcharts.theme && Highcharts.theme.background2) || 'white',
        borderColor: '#CCC',
        borderWidth: 1,
        shadow: false
    },
    tooltip: {
        headerFormat: '<b>{point.x}</b><br/>',
        pointFormat: '{series.name}: {point.y}<br/>Total: {point.stackTotal}'
    },
    plotOptions: {
        series: {
            pointWidth: bar_width,
            enableMouseTracking: false,
              shadow: false, 
              animation: false
        },
        column: {
            stacking: 'normal',
            dataLabels: {
                enabled: true,
                color: "#000",
                style:{
                    fontSize: label_inner_size
                }
            }
        }
    },
    series: data.series
  });
</script>
