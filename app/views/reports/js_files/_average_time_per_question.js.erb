var container = $("#avg-time-per-question-graph-<%= sec_name_id %>")[0];
Highcharts.chart(container, {
    chart: {
        plotBackgroundColor: null,
        plotBorderWidth: null,
        plotShadow: false,
        type: 'pie',
        backgroundColor: chart_background
    },
    title: {
        text: '<b><%= sec_name %></b>',
        style:{
            color: "#5e5e5e"
        }
    },
    tooltip: {
        formatter: function(){
            return ' <b>' + this.series.name + '</b>: <b>' + this.point.avg_time + '</b>'
        }
    },
    plotOptions: {
        pie: {
            allowPointSelect: true,
            cursor: 'pointer',
            dataLabels: {
                enabled: false
                // format: '<b>{point.name}</b>: {point.percentage:.0f} %',
                // style: {
                //     color: (Highcharts.theme && Highcharts.theme.contrastTextColor) || 'black'
                // }
            }
        },
        series: {
          enableMouseTracking: false,
          shadow: false, 
          animation: false
        }
    },
    series: [{
        name: "<%= sec_name %>",
        colorByPoint: true,
        data: JSON.parse('<%=raw section_data.to_json %>')
    }]
});
