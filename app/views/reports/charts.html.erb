<style type="text/css">
  .report-container{
    font-family: 'LatoRegular';;
  }
  .report-section-container .report-section-container-inner{
    border-bottom: 1px solid #efefef;
    margin-bottom: 60px;
  }
  .report-section-heading{
    text-transform: uppercase;
    color: #2196f3;
    font-size: 26px;
    border-bottom: 1px solid #efefef;
    margin-bottom: 20px;
  }
  .box-1{
    display: inline-block;
  }
  .box-1 > div:first-child{
    text-transform: uppercase;
    font-size: 18px;
    font-weight: bold;
    width: 59px;
    height: 35px;
    line-height: 1.08;
    color: #3e3e3e;
    float: left;
  }
  .box-1 > div:last-child{
    margin-left: 7px;
    width: 52px;
    height: 35px;
    font-size: 45.9px;
    float: left;
    line-height: 0.8;
  }
  .box-2{
    width: 170px;
    height: 38px;
    background-color: #2196f3;
    font-size: 23px;
    text-align: center;
    color: #fff;
    line-height: 1.7;
    font-weight: 600;
    padding-left: 0px;
  }

  .box-3{
    display: inline-block;
    width: 100%;
    background-color: #efefef;
    padding: 10px;
  }

  .box-3 > div:first-child{
    width: 140px;
    height: 35px;
    font-size: 18px;
    float: left;
    line-height: 18px;
    letter-spacing: 1px;
  }

  .box-3 > div:last-child{
    float: right;
  }

  .text-s-1{
    font-size: 11px;
  }
  .text-s-5{
    font-size: 25px;
  }

  .box-3 .text-s-5{
    height: 20px;
    line-height: 23px;
  }

  .box-3 .text-s-1{
    text-align: right;
    letter-spacing: 2px;
  }

  .explanation-head{
    font-size: 16px;
    margin-top: 10px;
    font-weight: 700;
  }
  .explanation-text{
    max-width: 250px;
    text-align: justify;
  }
  .color-green{
    color: #56a062;
  }
  .color-g-orange{
    color: #e3773d
  }
  .color-blue{
    color: #2196f3;
  }
  .note{
    color: #ff0000;
  }
  .percent-correct-incorrect .box-1:first-child{
    margin-bottom: 10px;
  }
  .percent-correct-incorrect .box-1 > div:first-child{
    width: 109px;
  }
  .percent-text{
    font-size: 28px;
  }
  .percent-correct-incorrect .box-3 > div:first-child{
    height: 17px;
  }
  .percent-correct-incorrect .box-3 .percent-text{
    font-size: 16px;
    font-weight: 900;
  }
  .section-correct-incorrect .box-3 > div:first-child {
    height: 36px;
  }
  .section-correct-incorrect .box-3 .text-s-5{
    margin-top: 8px;
  }
  .section-correct-incorrect .box-3:last-child .text-s-5{
    margin-top: 0px;
  }
  .section-correct-incorrect .box-3:last-child > div:first-child{
    height: 21px;
  }

  .avg-time-per-set .box-3 > div:first-child{
    line-height: 35px;
  }

  .avg-time-correct-incorrect .text-s-5{
    width: 65px;
    height: 19px;
    line-height: 22px;
    font-size: 28px;
  }
  .avg-time-correct-incorrect .text-s-1{
    font-size: 13px;
    margin-top: 6px;
    width: 62px;
    text-align: right;
  }
  .avg-time-correct-incorrect .box-1 > div:first-child{
    width: 93px;
  }
  .avg-difficulty-per-set .box-3 > div:first-child{
    margin-top: 10px;
    height: 16px;
  }
  .avg-difficulty-per-set .box-3 > div > div:last-child{
    width: 60px;
    height: 35px;
  }
  .avg-difficulty-per-set .low, .avg-difficulty-per-set .medium, .avg-difficulty-per-set .high{
    padding-top: 10px;
  }
</style>
<%= content_for :main_container_classes do%>report-container<% end %>
<script type="text/javascript">
  var bar_colors = JSON.parse('<%=raw @paper.bar_colors.to_json.html_safe %>');
  var chart_background = 'rgba(255, 255, 255, 0.1)';
  var label_inner_size = "11px";
</script>
<%= render "reports/verbal_section_performance" %>
<%= render "reports/sub_section_time_management" %>
<%= render "reports/percent_correct_incorrect" %>
<%= render "reports/progress" %>
<% @section_correct_incorrect_data = @paper.section_percentage_correct_incorrect_data %>
<% @average_time_per_question = @paper.average_time_per_question %>
<%= render "reports/section_correct_incorrect" %>
<%= render "reports/average_time_per_question" %>
<%= render "reports/average_time_correct_incorrect" %>
<%= render "reports/average_difficulty_level" %>
<%= content_for :bottom_js do %>
  <% javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.6.0/Chart.bundle.min.js" %>
  <%= javascript_include_tag "https://code.highcharts.com/highcharts.src.js" %>
  <%= render "reports/verbal_section_performance_js" %>
  <%= render "reports/sub_section_time_management_js" %>
  <%= render "reports/percent_correct_incorrect_js" %>
  <%= render "reports/progress_js" %>
  <% @section_correct_incorrect_data.each do |section_data| %>
    <% sec_name_id = section_data["section_name"].split(" ").join("-") %>
    <%= render "reports/section_correct_incorrect_js", {sec_name: section_data["section_name"], sec_name_id: sec_name_id, section_data: section_data["data"]} %>
  <% end %>
  <script type="text/javascript">
    <% @average_time_per_question.each do |section_data| %>
      <% sec_name_id = section_data["section_name"].split(" ").join("-") %>
      <%= render partial: 'reports/average_time_per_question', locals: {sec_name: section_data['section_name'], sec_name_id: sec_name_id, section_data: section_data['data']}, formats: [:js] %>
    <% end %>
    <%= render partial: 'reports/average_time_correct_incorrect', formats: [:js] %>
    <%= render partial: 'reports/average_difficulty_level', formats: [:js] %>
  </script>
<% end %>
