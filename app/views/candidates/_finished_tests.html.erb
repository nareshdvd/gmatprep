<% if finished_tests.present? %>
  <div>
    <table class="table table-bordered table-striped">
      <thead>
        <tr>
          <th>Test #</th>
          <th>Report</th>
          <th>Graphs</th>
        </tr>
      </thead>
      <% finished_tests.each_with_index do |paper, inx| %>
        <tr>
          <td><%= inx + 1 %></td>
          <td><%= link_to "Report", report_index_path(paper.id), target: "_blank" %></td>
          <td><%= link_to "Graphs", report_charts_path(paper.id), target: "_blank" %></td>
        </tr>
      <% end %>
    </table>
  </div>
<% end %>