<% if notice || alert %>
  <div class="row margin-b-10">
    <div class="col-sm-12">
      <% if notice %>
        <p class="notice alert-success"><%= notice %></p>
      <% end %>
      <% if alert %>
        <p class="alert alert-success"><%= alert %></p>
      <% end %>
    </div>
  </div>
<% end %>