@font-face {
  font-family: 'LatoThin';
  src: url('/../app/assets/fonts/lato/thin/Lato.eot');
  src: url('/../app/assets/fonts/lato/thin/Lato.eot?iefix') format('eot'),
      url('/../app/assets/fonts/lato/thin/Lato.woff') format('woff'),
      url('/../app/assets/fonts/lato/thin/Lato.ttf') format('truetype');
}

